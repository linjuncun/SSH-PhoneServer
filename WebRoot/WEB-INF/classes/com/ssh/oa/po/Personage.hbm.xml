<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ssh.oa.po">

	<class name="Personage" table="personage">
		<id name="id">
            <generator class="native"/>
		</id>
		
		<property name="name" />	
		<property name="sex"/>
		<property name="company"/>
		<property name="number"/>
		<property name="inviter"/>
		<property name="photoName"></property>
	</class>
	
</hibernate-mapping>
